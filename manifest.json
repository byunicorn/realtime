{
	"name": "realtime",
	"version": "1.0",
	"manifest_version": 2,
	"browser_action": {
        "default_icon": {
            "128": "images/icon128.png"
        },
        "default_title": "realtime data helper",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
    	"matches": ["file:///E:/*", "https://item.taobao.com/*"],
    	"css": ["style.css"],
    	"js": ["jquery.js", "main.js"]
    }],
    "background": {
    	"scripts": ["jquery.js", "background.js"],
    	"persistent": false
    },
    "permissions": [
    	"tabs",
    	"https://item.taobao.com/*"
    ]
}